{
  "name": "store-manager",
  "version": "1.0.0",
  "description": "A RESTful API for store Manager",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=lcov --reporter=text mocha --require @babel/polyfill --require @babel/register ./tests/*.spec.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "node dist/index.js",
    "postinstall": "npm run build",
    "start-dev": "nodemon --exec babel-node --presets=@babel/env v1/index.js",
    "build": "rm -rf ./dist && babel v1 --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iAfolayan/store-manager.git"
  },
  "keywords": [
    "Store",
    "Manager"
  ],
  "author": "Afolayan Isaiah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iAfolayan/store-manager/issues"
  },
  "homepage": "https://github.com/iAfolayan/store-manager#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-literals": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/plugin-transform-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/runtime": "^7.1.2",
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-messages": "^1.0.1",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nyc": "^13.0.0"
  }
}
